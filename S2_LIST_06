def active_user_names(users):
    return sorted([u["name"].upper() for u in users if u.get("active")])

# Тест
users = [
    {"id":1,"name":"Ola","active":True},
    {"id":2,"name":"Bob","active":False},
    {"id":3,"name":"Alice","active":True}
]
print(active_user_names(users))
# → ['ALICE','OLA']
